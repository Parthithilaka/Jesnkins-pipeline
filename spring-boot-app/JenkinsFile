pipeline {
agent any
    tools {
        maven 'maven'
    }
    stages {
        stage ('scm') {
            steps {
                echo "image pulled from docekrhub and version is ${params.version}"
            }
        }
        stage ('build') {
            steps {
                sh 'cd spring-boot-app && mvn clean package '
            }
        }
    }
}
